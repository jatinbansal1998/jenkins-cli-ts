{
  "name": "jenkins-cli-ts",
  "type": "module",
  "private": true,
  "module": "src/index.ts",
  "bin": {
    "jenkins-cli": "dist/index.js"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "install:global": "bun run build && bun install -g \"file:.\"",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/yargs": "^17.0.35",
    "oxlint": "^1.42.0",
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "yargs": "^18.0.0"
  }
}
